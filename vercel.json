{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/src/index.js"
        }
    ],
    "functions": {
        "src/*.js": {
            "maxDuration": 30
        },
        "src/Controllers/*.js": {
            "maxDuration": 30
        },
        "./**/*.js":{
            "maxDuration": 30
        },
        "**/*.js":{
            "maxDuration": 30
        }
    }
}