{
    "version": 2,
    "functions": {
      "api/*.js": {
        "runtime": "nodejs18.x",
        "maxDuration": 30
      },
      "src/Controllers/*.js": {
        "runtime": "nodejs18.x",
        "maxDuration": 30
      },
      "src/*.js": {
        "runtime": "nodejs18.x",
        "maxDuration": 30
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/index.js"
      }
    ]
  }